<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY gen-framework "http://kmi.open.ac.uk/task-method#" >
    <!ENTITY daml "http://www.daml.org/2001/03/daml+oil#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY simmetrics "http://kmi.open.ac.uk/fusion/simmetrics#" >
    <!ENTITY provenance "http://kmi.open.ac.uk/fusion/provenance#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY opus "http://lsdis.cs.uga.edu/projects/semdis/opus#" >
    <!ENTITY protege "http://protege.stanford.edu/plugins/owl/protege#" >
]>


<rdf:RDF xmlns="http://kmi.open.ac.uk/fusion/fusion#"
     xml:base="http://kmi.open.ac.uk/fusion/fusion"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:daml="http://www.daml.org/2001/03/daml+oil#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:provenance="http://kmi.open.ac.uk/fusion/provenance#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:simmetrics="http://kmi.open.ac.uk/fusion/simmetrics#"
     xmlns:gen-framework="http://kmi.open.ac.uk/task-method#"
     xmlns:opus="http://lsdis.cs.uga.edu/projects/semdis/opus#">
    <owl:Ontology rdf:about="http://kmi.open.ac.uk/fusion/fusion"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&rdfs;comment"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://kmi.open.ac.uk/fusion/fusion#appearsInSet -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#appearsInSet">
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#ConflictDescription"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#attribute -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#attribute">
        <rdfs:comment>Attribute, whose value is used for instance matching. Normally, this property is used by the VariableComparisonSpec object: in this case, the value of this attribute is used directly by the decision rule. Also, when using a genetic algorithm to find an optimal decision rule, one can specify additional attributes for the ApplicationContext: in this way, these attributes would be added to the pool. Note that this property is not used directly: its subproperties, sourceAttribute and targetAttribute should be used instead.</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#ApplicationContext"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#VariableComparisonSpec"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#conflictsWith -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#conflictsWith"/>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#containsConflictingStatements -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#containsConflictingStatements"/>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#containsInstance -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#containsInstance">
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#FusionSet"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#datasetMatchingSpec -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#datasetMatchingSpec">
        <rdfs:comment>Points to the ApplicationContext object which describes an application of a dataset matching method. Now, the link session first applies all object identification methods (ApplicationContext object linked via the instanceMatchingSpec property) and then all dataset matching methods (ApplicationContext object linked via the datasetMatchingSpec property)</rdfs:comment>
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#ApplicationContext"/>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LinkSession"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#hasAttribute -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#hasAttribute">
        <rdfs:comment>Specifies the attribute which serves as an argument for another attribute (composite or transformation). </rdfs:comment>
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#Attribute"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#CompositeAttribute"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#TransformationAttribute"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#hasBlocker -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#hasBlocker">
        <rdfs:comment>Specifies a blocker for an instance matching ApplicationContext.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#ApplicationContext"/>
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#Blocker"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#hasCapability -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#hasCapability">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>Denotes a capability of the method: either ObjectIdentification or DatasetMatching.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#FusionMethod"/>
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#MethodCapability"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#hasMethod -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#hasMethod">
        <rdfs:comment>Specifies the method used in the ApplicationContext.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#ApplicationContext"/>
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#FusionMethod"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#instanceMatchingSpec -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#instanceMatchingSpec">
        <rdfs:comment>Points to the ApplicationContext object which describes an application of an instance matching method. Now, the link session first applies all object identification methods (ApplicationContext object linked via the instanceMatchingSpec property) and then all dataset matching methods (ApplicationContext object linked via the datasetMatchingSpec property)</rdfs:comment>
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#ApplicationContext"/>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LinkSession"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#intermediateStore -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#intermediateStore">
        <rdfs:comment>Specifies the intermediate data store for the data source. This intermediate store is assumed to provide a faster access (usually used to support blocking). If an intermediate data store is provided, then selected data is copied from the original to the intermediate store.</rdfs:comment>
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#Blocker"/>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#DataSource"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#loadFrom -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#loadFrom">
        <rdfs:comment>If data are to be reloaded, specifies the set of dump files.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#DataSource"/>
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#Dump"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#producedBy -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#producedBy">
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#FusionMethod"/>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#FusionObject"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#sourceAttribute -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#sourceAttribute">
        <rdfs:comment>Attribute from the source repository, whose value is used for instance matching. </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#attribute"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#sourceDataset -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#sourceDataset">
        <rdfs:comment>Source dataset for the link session. Normally, it is assumed that the source dataset is a smaller one. </rdfs:comment>
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#DataSource"/>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LinkSession"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#targetAttribute -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#targetAttribute">
        <rdfs:comment>Attribute from the target repository, whose value is used for instance matching. </rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#attribute"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#targetDataset -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#targetDataset">
        <rdfs:comment>Target dataset for the link session. Normally, it is assumed that the target dataset is a larger one. </rdfs:comment>
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#DataSource"/>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LinkSession"/>
    </owl:ObjectProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#variableComparison -->

    <owl:ObjectProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#variableComparison">
        <rdfs:comment>Comparison between two attributes which produces a degree of similarity between them. Aggregated similarities are then used by the decision rule.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#ObjectContextModel"/>
        <rdfs:range rdf:resource="http://kmi.open.ac.uk/fusion/fusion#VariableComparisonSpec"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topObjectProperty -->

    <owl:ObjectProperty rdf:about="&owl;topObjectProperty"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://kmi.open.ac.uk/fusion/fusion#abbreviationsFile -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#abbreviationsFile">
        <rdfs:comment>File containing the domain-specific aliases and abbreviations. Each line of the file should have the following format:
&lt;canonic name&gt;, &lt;alias1&gt;, &lt;alias2&gt;
e.g.:
Co, Corp, Corporation, Co
</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LinkSession"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#aggregationFunction -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#aggregationFunction">
        <rdfs:comment>Aggregation function combining the attribute-based similarities: average or max.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#ObjectContextModel"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#aligned -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#aligned">
        <rdfs:comment>Specifies whether the genetic algorithm should only try to compare values of the same property (e.g., if the datasets use the same schema) or any properties. </rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#GeneticAlgorithmObjectIdentificationMethod"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#attributeType -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#attributeType">
        <rdfs:comment>Attribute data type. One of: string, nominal, double, composite, text, date, integer. The difference between string, nominal, and text is mainly in the length. Nominal attribute values are treated as a single token. String ones can consist of several tokens which can be handled separately. E.g., Jaccard set similarity can be applied to string attributes but not to nominal ones. Text attributes represent long pieces of text: they can be compared on the basis of words (e.g., using the Jaccard score), but not characters (e.g., Jaro similarity is not applicable). If attributeType value is not specified, the system assignes a type automatically based on the string length and format of attribute values.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#Attribute"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#beta -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#beta">
        <rdfs:comment>Only applicable to the genetic algorithm method. Specifies the beta coefficient in the F-Measure which denotes the relative importance of recall over precision. The default value is 0.1. The value 1.0 should be used if it is known that all instances in the smaller repository should have mapping counterparts in the bigger one.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#GeneticAlgorithmObjectIdentificationMethod"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#configurationFile -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#configurationFile">
        <rdfs:comment>Sesame SAIL configuration file</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#SesameDataSource"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#criterion -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#criterion">
        <rdfs:comment>Only applicable to the genetic algorithm method. Fitness criterion used to select the best candidate solution. Can be one of &#39;pseudo-f-measure&#39; (default) or &#39;neighbourhood growth&#39;.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#GeneticAlgorithmObjectIdentificationMethod"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#crossoverRate -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#crossoverRate">
        <rdfs:comment>Crossover rate of the genetic algorithm. Default value is 0.3.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#GeneticAlgorithmObjectIdentificationMethod"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#cutOff -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#cutOff">
        <rdfs:comment>The cut off threshold for a blocker. The number of candidate instances for a source individual will not be greater than this cut off value.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LuceneBlocker"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#dataFile -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#dataFile">
        <rdfs:comment>A file containing RDF data dump. The format is determined based on the extension: .rdf, .nt, or .n3.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#FileDump"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#depth -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#depth">
        <rdfs:comment>Specifies the maximum length of the property path between the instance and attribute values. Used by the blocker (all datatype properties within the depth are included into the index) as well as by the genetic algorithm method (all datatype properties within the depth are included into the property pool).</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#GeneticAlgorithmObjectIdentificationMethod"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#LuceneBlocker"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#fuzzyThreshold -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#fuzzyThreshold">
        <rdfs:comment>Used in case where the Lucene blockers uses fuzzy search (searchStrategy = &quot;fuzzy&quot;). This is the minimal required edit distance similarity between the query term and the indexed property value.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LuceneBlocker"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#goldStandard -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#goldStandard">
        <rdfs:comment>Path to the file containing the gold standard mappings (for experimental testing). The mappings can be described using the custom KnoFuss XML format, the standard OAEI alignment API format, or as owl:sameAs links in an arbitrary RDF file.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#ApplicationContext"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#LinkSession"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#hasImplementingClass -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#hasImplementingClass">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>The name of the Java class realising the behaviour of the corresponding CustomFusionObject (e.g., a method, a transformation function, or a value similarity function).</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#CustomFusionObject"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#hasReliability -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#hasReliability">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:comment>Denotes the reliability of a fusion method.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;float"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#ApplicationContext"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#FusionMethod"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#hasTransformationFunction -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#hasTransformationFunction">
        <rdfs:comment>Provides the name of the transformation function. This function is applied to the value of the argument attribute(s) and returns the value assigned to the custom transformation attribute. Note that the function is referred by its name (toString() property of the implementation class) rather than as an RDF resource.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#TransformationAttribute"/>
    </owl:DatatypeProperty>
    
	<!-- http://kmi.open.ac.uk/fusion/fusion#idFieldName -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#idFieldName">
        <rdfs:comment>Provides the name of the Lucene index field storing the URI of the instance. Default value: "uri".</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LuceneDiskBlocker"/>
    </owl:DatatypeProperty>

    <!-- http://kmi.open.ac.uk/fusion/fusion#maxIterations -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#maxIterations">
        <rdfs:comment>Only applicable to the genetic algorithm method. Specifies the maximal number of iterations of the genetic algorithm.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#GeneticAlgorithmObjectIdentificationMethod"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#metric -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#metric">
        <rdfs:comment>Specifies the similarity metric applied to compare two attribute values. In case of a non-standard metric, it is referred by its name (returned by the toString() property of the implementing class) and not by the RDF resource of the configuration object.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#VariableComparisonSpec"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#mutationRate -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#mutationRate">
        <rdfs:comment>Denotes the mutation rate of the genetic algorithm. Default value is 0.6.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#GeneticAlgorithmObjectIdentificationMethod"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#name -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#name">
        <rdfs:comment>Name of the Sesame repository (should match the one specified in the configuration RDF file).</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#SesameDataSource"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#optional -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#optional">
        <rdfs:comment>A boolean value specifying if the attribute is an optional one.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#Attribute"/>
        <rdfs:range rdf:resource="&xsd;boolean"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#path -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#path">
        <rdfs:comment>File or directory path on the disk</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#LuceneDiskBlocker"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#SesameDataSource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#populationSize -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#populationSize">
        <rdfs:comment>Only applicable to the genetic algorithm method. Specifies the size of the selection pool of candidate solutions.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#GeneticAlgorithmObjectIdentificationMethod"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#propertyPath -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#propertyPath">
        <rdfs:comment>Specifies the property path from the instance to the attribute value. Can be defined either as a set of SPARQL triple patterns (starting from the ?uri variable)

?uri temp:exampleProperty ?val . ?val skos:prefLabel ?name .

or as a property chain:

&lt;&amp;temp;exampleProperty&gt;/&lt;&amp;skos;prefLabel&gt;
</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="&owl;topDataProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#AtomicAttribute"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#VariableComparisonSpec"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#proxyHost -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#proxyHost">
        <rdfs:comment>Proxy host used when connecting to an external SPARQL endpoint.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#RemoteSPARQLDataSource"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#proxyPort -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#proxyPort">
        <rdfs:comment>Proxy port used when connecting to an external SPARQL endpoint.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#RemoteSPARQLDataSource"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#rdfsReasoning -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#rdfsReasoning">
        <rdfs:comment>A boolean value specifying whether a Sesame data source should use RDFS reasoning.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;boolean"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#SesameMemoryDataSource"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#SesameNativeDataSource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#refresh -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#refresh">
        <rdfs:comment>Specifies whether the data should be reloaded to the data source or blocker. Since this takes time, it makes sense to set refresh to true for the first run, and then change to false for subsequent runs, if the data source/blocker use disk storage.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;boolean"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#Blocker"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#DataSource"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#restriction -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#restriction">
        <rdfs:comment>SPARQL restriction specifying which instances should be selected for processing. Normally includes a restriction of the instance type of the form:
?uri rdf:type foaf:Person. The variable name ?uri is reserved to denote the actual instance to be tested for linking. The property &#39;restriction&#39; is not used directly: instead, its subproperties &#39;sourceRestriction&#39; and &#39;targetRestriction&#39; are declared.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#ApplicationContext"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#resultsFile -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#resultsFile">
        <rdfs:comment>Path to the file where the produced links should be saved.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LinkSession"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#resultsFormat -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#resultsFormat">
        <rdfs:comment>Format of the resulting links. Should be one of &#39;oaei&#39; (OAEI Alignment API format), &#39;sameas&#39; (RDF file with owl:sameAs links) or &#39;custom&#39; (custom KnoFuss XML format).</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LinkSession"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#sampleSize -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#sampleSize">
        <rdfs:comment>Only applicable to the genetic algorithm method. Specifies the size of the random sample of the source dataset to be used for training.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#GeneticAlgorithmObjectIdentificationMethod"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#schemaFile -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#schemaFile">
        <rdfs:comment>A file containing schema for the RDF dump. The format is determined based on the extension: .owl, .rdf, .nt, or .n3.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#FileDump"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#searchStrategy -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#searchStrategy">
        <rdfs:comment>Search strategy used by the lucene indexer for blocking. Can be one of &#39;all&#39; (having an attribute value of a source instance, searches for it among all properties of indexed target instances), &#39;aligned&#39; (searches only among the values of the same attribute - useful if the source and target datasets use the same schema), &#39;fuzzy&#39; (same as &#39;all&#39;, but using Lucene fuzzy search).</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LuceneBlocker"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#sourcePath -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#sourcePath">
        <rdfs:comment>Property path in the source dataset.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#VariableComparisonSpec"/>
        <rdfs:subPropertyOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#propertyPath"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#sourceRestriction -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#sourceRestriction">
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#ApplicationContext"/>
        <rdfs:subPropertyOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#restriction"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#targetPath -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#targetPath">
        <rdfs:comment>Property path in the target dataset.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#VariableComparisonSpec"/>
        <rdfs:subPropertyOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#propertyPath"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#targetRestriction -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#targetRestriction">
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#ApplicationContext"/>
        <rdfs:subPropertyOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#restriction"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#threshold -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#threshold">
        <rdfs:comment>The similarity score threshold. Can apply to the blocking stage (in which case denotes the minimal Lucene search score for an instance to be considered a mapping candidate) or to the instance matching stage (denotes the threshold for the aggregated similarity value).</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;double"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#LuceneBlocker"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#ObjectContextModel"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#url -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#url">
        <rdfs:comment>URL of the SPARQL endpoint provided by an external repository.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#RemoteSPARQLDataSource"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#useUnsupervisedFitness -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#useUnsupervisedFitness">
        <rdfs:comment>Only applicable to the genetic algorithm method. Specifies whether to use the unsupervised or supervised learning (if some training data is available, the latter is preferred).</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#variable -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#variable">
        <rdfs:comment>Name of the variable which denotes the value of the attribute to be compared. This variable name should be the one used in the sourcePath/targetPath expressions.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#AtomicAttribute"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#CompositeAttribute"/>
                    <rdf:Description rdf:about="http://kmi.open.ac.uk/fusion/fusion#VariableComparisonSpec"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#weight -->

    <owl:DatatypeProperty rdf:about="http://kmi.open.ac.uk/fusion/fusion#weight">
        <rdfs:comment>Weight of the similarity between two attributes.</rdfs:comment>
        <rdfs:domain rdf:resource="http://kmi.open.ac.uk/fusion/fusion#VariableComparisonSpec"/>
    </owl:DatatypeProperty>
    


    <!-- http://kmi.open.ac.uk/fusion/provenance#hasTimestamp -->

    <owl:DatatypeProperty rdf:about="&provenance;hasTimestamp">
        <rdfs:range rdf:resource="&xsd;dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/2002/07/owl#topDataProperty -->

    <owl:DatatypeProperty rdf:about="&owl;topDataProperty"/>
    


