    <!-- http://kmi.open.ac.uk/fusion/fusion#LuceneDiskBlocker -->

    <owl:Class rdf:about="http://kmi.open.ac.uk/fusion/fusion#LuceneDiskBlocker">
        <rdfs:subClassOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LuceneBlocker"/>
    </owl:Class>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#LuceneDiskBlockerAllFields -->

    <owl:Class rdf:about="http://kmi.open.ac.uk/fusion/fusion#LuceneDiskBlockerAllFields">
        <rdfs:subClassOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LuceneBlocker"/>
