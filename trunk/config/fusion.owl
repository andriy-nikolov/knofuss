    <!-- http://kmi.open.ac.uk/fusion/fusion#LuceneMemoryBlocker -->

    <owl:Class rdf:about="http://kmi.open.ac.uk/fusion/fusion#LuceneMemoryBlocker">
        <rdfs:subClassOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LuceneBlocker"/>
    </owl:Class>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#LuceneMemoryBlockerAllFields -->

    <owl:Class rdf:about="http://kmi.open.ac.uk/fusion/fusion#LuceneMemoryBlockerAllFields">
        <rdfs:subClassOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#LuceneBlocker"/>
    </owl:Class>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#Mapping -->

    <owl:Class rdf:about="http://kmi.open.ac.uk/fusion/fusion#Mapping">
        <rdfs:subClassOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#FusionSet"/>
