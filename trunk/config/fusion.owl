    <!-- http://kmi.open.ac.uk/fusion/fusion#ObjectIdentificationMethod -->

    <owl:Class rdf:about="http://kmi.open.ac.uk/fusion/fusion#ObjectIdentificationMethod">
        <rdfs:subClassOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#FusionMethod"/>
