    </owl:Class>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#MethodCapability -->

    <owl:Class rdf:about="http://kmi.open.ac.uk/fusion/fusion#MethodCapability"/>
    


    <!-- http://kmi.open.ac.uk/fusion/fusion#ObjectContext -->

    <owl:Class rdf:about="http://kmi.open.ac.uk/fusion/fusion#ObjectContext">
        <rdfs:subClassOf rdf:resource="http://kmi.open.ac.uk/fusion/fusion#DataInstance"/>
